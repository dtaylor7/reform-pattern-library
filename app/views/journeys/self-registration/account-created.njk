{% set title = pageTitle %}
{% extends "layout.njk" %}

{% block page_title %}{{pageTitle}} - GOV.UK{% endblock %}

{% block content %}
<main class="pattern-library" id="content" role="main">

  {% include "partials/phase_banner.njk" ignore missing %}

  {% include "partials/breadcrumb.njk" ignore missing %}

  <div class="grid-row">

    <!-- account created -->
    <div class="column-two-thirds">

      <h1 class="heading-large">Account created</h1>
      
      <p>You can now sign in to your account.</p>
      
      <p><a class="button" href="/journeys/self-registration/sign-in" role="button">Continue</a></p>
    
    </div>

  </div>

</main>
{% endblock %}
